{
  "name": "svelte-highlight",
  "version": "6.2.0",
  "license": "MIT",
  "description": "Syntax Highlighting for Svelte using highlight.js",
  "author": "Eric Liu (https://github.com/metonym)",
  "sideEffects": [
    "src/styles/*.css"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:lib": "node scripts",
    "package": "yarn build:lib && svelte-kit package",
    "check": "svelte-check --workspace tests",
    "test": "vitest",
    "format": "prettier --ignore-path .gitignore --write ."
  },
  "dependencies": {
    "highlight.js": "11.6.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0-next.39",
    "@sveltejs/kit": "1.0.0-next.405",
    "@testing-library/svelte": "^3.1.3",
    "@testing-library/user-event": "^14.4.2",
    "@types/prettier": "^2.7.0",
    "carbon-components-svelte": "^0.67.6",
    "carbon-icons-svelte": "^11.2.0",
    "carbon-preprocess-svelte": "^0.9.1",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-focus-key": "^0.3.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.13",
    "totalist": "^3.0.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.21.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/svelte-highlight.git"
  },
  "homepage": "https://github.com/metonym/svelte-highlight",
  "bugs": "https://github.com/metonym/svelte-highlight/issues",
  "keywords": [
    "svelte",
    "svelte 3",
    "svelte components",
    "highlight.js",
    "highlight",
    "code",
    "syntax",
    "typescript"
  ]
}
