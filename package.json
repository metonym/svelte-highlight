{
  "name": "svelte-highlight",
  "version": "7.3.0",
  "license": "MIT",
  "description": "Syntax Highlighting for Svelte using highlight.js",
  "author": "Eric Liu (https://github.com/metonym)",
  "sideEffects": [
    "src/styles/*.css"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:lib": "node scripts",
    "package": "yarn build:lib && svelte-package",
    "check": "svelte-check --workspace tests",
    "test": "vitest",
    "format": "prettier --ignore-path .gitignore --write ."
  },
  "dependencies": {
    "highlight.js": "11.8.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "2.0.2",
    "@sveltejs/kit": "1.22.0",
    "@sveltejs/package": "^1.0.2",
    "@testing-library/svelte": "^4.0.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/prettier": "^2.7.3",
    "carbon-components-svelte": "^0.76.0",
    "carbon-icons-svelte": "^11.4.0",
    "carbon-preprocess-svelte": "^0.9.1",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.4",
    "svelte-focus-key": "^1.0.0",
    "svelte2tsx": "^0.6.16",
    "totalist": "^3.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/svelte-highlight.git"
  },
  "homepage": "https://github.com/metonym/svelte-highlight",
  "bugs": "https://github.com/metonym/svelte-highlight/issues",
  "keywords": [
    "svelte",
    "svelte 3",
    "svelte components",
    "highlight.js",
    "highlight",
    "code",
    "syntax",
    "typescript"
  ]
}
